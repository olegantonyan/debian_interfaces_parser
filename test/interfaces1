auto lo
iface lo inet loopback

auto eth0:0
iface eth0:0 inet static
        address 10.10.10.2
        netmask 255.255.255.252

auto eth0
iface eth0 inet static
        address 192.168.1.170
        netmask 255.255.255.0
        gateway 192.168.1.1
        dns-nameservers 192.168.1.1, 8.8.8.8

auto wlan0
iface wlan0 inet dhcp
    wpa-ssid hello
    wpa-psk hello_123
